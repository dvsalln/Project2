<h1>Hello {{firstName}}</h1>

<div class="container profileContainer">
    <div class="row">
        <div class="col-sm-3">
            <img src="data:image/png;base64, {{profilePhoto}}" alt="Profile Photo" class="img-thumbnail">/>
        </div>

        <div class="col-sm-9">
            <div class="container">
                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>First Name</strong>: {{firstName}}</p>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Last Name</strong>: {{lastName}}
                            </p>

                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <p><strong>Email</strong>: {{email}}
                    </p>
                </div>

                <div class="row">
                    <div class="col-sm-6">
                        <p><strong>Age</strong>: {{age}}
                        </p>
                    </div>

                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Gender</strong>: {{gender}}
                            </p>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Occupation</strong>: {{occupation}}
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <p><strong>Location</strong>: {{location}}
                        </p>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Tell us something interesting about yourself</strong>: {{summary}}
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Interests/hobbies/goals</strong>: {{interests}}
                            </p>
                        </div>
                    </div>
                </div>

                {{!-- <button type="submit" class="btn btn-primary editProfile">Edit</button> --}}

            </div>
        </div>
    </div>
</div>

<script src="" async defer></script>