<h1>Hello {{profile.fName}}</h1>

<div class="container profileContainer">
    <div class="row">
        <div class="col-sm-3">
            <img src="..." alt="..." class="img-thumbnail">
        </div>

        <div class="col-sm-9">
            <div class = "container">
                <div class = "row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>First Name</strong>: {{profile.fName}}</p>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Last Name</strong>: {{profile.lName}}
                            </p>
                            
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <p><strong>Email</strong>: {{profile.email}}
                    </p>
                </div>

                <div class="row">
                    <div class="col-sm-6">
                        <p><strong>Age</strong>: {{profile.age}}
                        </p>
                    </div>
                    
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Gender</strong>: {{profile.gender}}
                            </p>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Occupation</strong>: {{profile.occupation}}
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <p><strong>Location</strong>: {{profile.location}}
                        </p>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Tell us something interesting about yourself</strong>: {{profile.userSummary}}
                            </p>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <p><strong>Interests/hobbies/goals</strong>: {{profile.userInterests}}
                            </p>
                        </div>
                    </div>
                </div>

                <button type="submit" class="btn btn-primary editProfile">Edit</button>

            </div>
        </div>
    </div>
</div>

<script src="" async defer></script>
